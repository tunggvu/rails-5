<div class= "col-md-6 col-md-offset-3">
<h1 class= "col-md-6 col-md-offset-3">Login</h1>

<%= form_tag({controller: 'sessions', action: "create"}, method: :post, class: "col-md-6 col-md-offset-3") do %>

    <%= label_tag :email %>
    <%= text_field_tag :email, nil, class: 'form-control' %>

    <%= label_tag :password %>
    <%= password_field_tag :password, nil, class: 'form-control'%>

    <br />
    <%= submit_tag "Login" %>
<% end %>
</div>>
